{
    "name": "nafigator/php-qa-hooks",
    "description": "Git hooks for PHP commits QA.",
    "type": "library",
    "license": "MIT",
    "version": "0.1.0",
    "authors": [{
        "name": "Yancharuk Alexander",
        "role": "developer"
    }],
    "support": {
        "issues": "https://github.com/nafigator/git-hooks/issues",
        "source": "https://github.com/nafigator/git-hooks"
    },
    "minimum-stability": "dev",
    "autoload": {
        "exclude-from-classmap": ["/src/", "/.images/"]
    },
    "require-dev": {
        "nafigator/bash-helpers": "0.*",
        "squizlabs/php_codesniffer": "3.*",
        "neronmoon/scriptsdev": "0.*"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "extra": {
        "scripts-dev": {
            "post-install-cmd": "hooks-install.sh"
        }
    },
    "keywords": [
        "git",
        "hooks",
        "bash",
        "scripts",
        "syntax",
        "conflicts",
        "qa",
        "php"
    ]
}
